{
  "name": "lyrics-finder",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.2",
    "lodash": "^4.17.20",
    "node-sass": "^4.14.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-scripts": "4.0.1",
    "react-toastify": "^7.0.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage --watchAll=false --testPathIgnorePatterns=src/utility/index.js",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
